/* Append a timer to the task */
void append_timeout(tcb_t *ptcb, STATUS (*pfxn)(uint8 evt, void *parg1, void *parg2), uint16 tout) {
	tic_t *ptic;

	if( tout == WAIT_FOREVER )
		return;

	ptic = &ptcb->tic;
	ptic->flags	|= TMR_ONE_SHOT;
	ptic->evt = 0;
	ptic->parg1 = ptcb;
	ptic->parg2 = 0;
	
	ptic->abs = tout;
	ptic->pfxn = pfxn;		
	timer_enqueue( ptic );
}
